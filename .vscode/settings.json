{
    "editor.fontSize": 16,
    "editor.fontFamily": "monospace",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit",
      "source.fixAll.stylelint": "explicit",
      "source.fixAll": "explicit"
    },
    "stylelint.validate": ["css", "scss", "html", "vue", "astro"],
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
      "html",
      "vue",
      "astro"
    ]
  }
  